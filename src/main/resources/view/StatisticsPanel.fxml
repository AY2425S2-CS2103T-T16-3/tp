<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.chart.BarChart?>
<?import javafx.scene.chart.CategoryAxis?>
<?import javafx.scene.chart.NumberAxis?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane xmlns:fx="http://javafx.com/fxml">
    <children>
        <BarChart fx:id="statsBarChart" layoutX="10.0" layoutY="10.0" prefHeight="400.0" prefWidth="600.0">
            <xAxis>
                <CategoryAxis fx:id="jobTitleAxis" label="Job Title"/>
            </xAxis>
            <yAxis>
                <NumberAxis fx:id="countAxis" label="Number of Applicants"/>
            </yAxis>
        </BarChart>
    </children>
</AnchorPane>
